    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/commander": "^2.12.2",
    "@types/ffbinaries": "^1.1.5",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
