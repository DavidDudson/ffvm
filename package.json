{
  "name": "ffvm",
  "version": "0.0.4",
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "ffbinaries": "^1.1.6",
